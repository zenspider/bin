#!/usr/local/bin/bash
#
# duReport - walks over all disks used by QA and reports usage on them.
#

echo "# This file was automatically generated using:"
echo '#   `'$0'`'

IFS=:

pushd / > /dev/null

for machine in $allMachines; do

  for directory in ${machine}*; do

    target=/${directory}/users/

    pushd $target > /dev/null

    echo ""
    echo "*** ${target}:"
    echo ""
    du -sk * | sort -rn

    popd > /dev/null

  done

done

popd > /dev/null
