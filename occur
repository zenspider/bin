#!/usr/bin/perl -w

use strict;
use Getopt::Long;

my %option = ();
GetOptions(\%option,
           "n",
           );

my %c = ();

while (<>) {
  chomp;

  s/(\d+)/<num>/g if $option{n};

  $c{$_}++;
}

foreach my $key (sort { $c{$b} <=> $c{$a} } keys %c) {
  printf "%5d: %s\n", $c{$key}, $key;
}
