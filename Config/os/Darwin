# -*-ksh-*-

OSPATH=/sbin:/usr/sbin:/bin:/usr/bin:/usr/local/sbin:/usr/local/bin:/opt/local/bin:/opt/local/sbin
if [ -d /usr/X11R6 ]; then
   OSPATH=$OSPATH:/usr/X11R6/bin
fi
export MANPATH=/usr/share/man:/usr/local/man:/usr/share/perl/man:/usr/local/lib/perl5/5.00503/man:/opt/local/man:/usr/X11R6/man:/opt/local/share/man

case $- in
  
  # interactive shell
  *i*)
       ;;
  *)
     ;;
esac

export JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/1.5/Home/
