# -*-ksh-*-

OSPATH=/sbin:/usr/sbin:/bin:/usr/bin:/usr/local/sbin:/usr/local/bin:/usr/X11R6/bin:/var/qmail/bin
export MANPATH=/usr/share/man:/usr/local/man:/usr/X11R6/man:/usr/share/perl/man:/usr/local/lib/perl5/5.00503/man:/usr/X11R6/man:/var/qmail/man

case $- in
  
  # interactive shell
  *i*)
       stty discard undef dsusp undef eol undef eol2 undef erase2 undef lnext undef quit undef reprint undef status undef werase undef
#       stty -iexten
       ;;
  *)
     ;;
esac
