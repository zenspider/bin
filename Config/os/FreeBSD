# -*-ksh-*-

OSPATH=/sbin:/usr/sbin:/bin:/usr/bin:/usr/local/sbin:/usr/local/bin:/usr/X11R6/bin
export MANPATH=/usr/share/man:/usr/local/man:/usr/X11R6/man:/usr/share/perl/man:/usr/local/lib/perl5/5.00503/man:/usr/X11R6/man

case $- in
  
  # interactive shell
  *i*)
       stty discard undef
       ;;
  *)
     ;;
esac
